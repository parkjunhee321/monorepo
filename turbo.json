{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "env": ["CI"],
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "build:app": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "deploy": {}
  }
}